#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_ioredis@5.8.1_reflect-metadata@0.2.2_sqlite3@5.1.7_7b1c417f625409f32ae9fee6e0d3e6c0/node_modules/typeorm/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_ioredis@5.8.1_reflect-metadata@0.2.2_sqlite3@5.1.7_7b1c417f625409f32ae9fee6e0d3e6c0/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_ioredis@5.8.1_reflect-metadata@0.2.2_sqlite3@5.1.7_7b1c417f625409f32ae9fee6e0d3e6c0/node_modules/typeorm/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_ioredis@5.8.1_reflect-metadata@0.2.2_sqlite3@5.1.7_7b1c417f625409f32ae9fee6e0d3e6c0/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
