#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_reflect-metadata@0.2.2_sqlite3@5.1.7_ts-node@10.9._af29d6ef0b8660388e10343a5a99cedd/node_modules/typeorm/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_reflect-metadata@0.2.2_sqlite3@5.1.7_ts-node@10.9._af29d6ef0b8660388e10343a5a99cedd/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_reflect-metadata@0.2.2_sqlite3@5.1.7_ts-node@10.9._af29d6ef0b8660388e10343a5a99cedd/node_modules/typeorm/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_reflect-metadata@0.2.2_sqlite3@5.1.7_ts-node@10.9._af29d6ef0b8660388e10343a5a99cedd/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
