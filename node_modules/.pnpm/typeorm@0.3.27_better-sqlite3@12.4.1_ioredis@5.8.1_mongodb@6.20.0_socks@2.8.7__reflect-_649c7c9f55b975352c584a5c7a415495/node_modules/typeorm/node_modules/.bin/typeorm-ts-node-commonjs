#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_ioredis@5.8.1_mongodb@6.20.0_socks@2.8.7__reflect-_649c7c9f55b975352c584a5c7a415495/node_modules/typeorm/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_ioredis@5.8.1_mongodb@6.20.0_socks@2.8.7__reflect-_649c7c9f55b975352c584a5c7a415495/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_ioredis@5.8.1_mongodb@6.20.0_socks@2.8.7__reflect-_649c7c9f55b975352c584a5c7a415495/node_modules/typeorm/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/typeorm@0.3.27_better-sqlite3@12.4.1_ioredis@5.8.1_mongodb@6.20.0_socks@2.8.7__reflect-_649c7c9f55b975352c584a5c7a415495/node_modules:/opt/projects/express_project_02/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-commonjs.js" "$@"
fi
